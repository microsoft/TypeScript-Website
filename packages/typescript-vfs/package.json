    "make-global": "node scripts/makeGlobals.js",
    "test": "dts test",
    "lint": "dts lint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".cache"
    ]
  },
  "browser": {
    "fs": false,
    "lz-string": false,
    "typescript": false
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "cpy-cli": "^3.1.1",
    "dts-cli": "^2.0.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
