  "main": "./dist/index.js",
  "module": "./dist/vfs.esm.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "dts watch",
    "bootstrap": "pnpm build",
    "build": "dts build && pnpm make-for-website && pnpm make-global",
